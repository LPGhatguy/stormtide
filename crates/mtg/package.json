{
  "name": "mtg-frontend",
  "version": "0.0.0",
  "devDependencies": {
    "mtg": "file:pkg",
    "parcel": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "scripts": {
    "build-wasm": "wasm-pack build --target web",
    "watch-wasm": "watchexec -- npm run build-wasm",
    "build": "npm run build-wasm && parcel build js/index.html",
    "serve": "npm run build-wasm && parcel serve js/index.html"
  }
}
